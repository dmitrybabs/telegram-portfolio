{
  "headers": [
    {
      "source": "/avatar.jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "images": {
    "minimumCacheTTL": 31536000,
    "formats": ["image/webp", "image/avif", "image/jpeg"]
  }
}